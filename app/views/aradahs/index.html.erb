  </thead>

  <tbody>
    <% @aradahs.each do |aradah| %>
      <tr>
        <td><%= aradah.user_id %></td>
        <td style="white-space: nowrap;"><%= aradah.date %></td>
        <td class="bg-light"><%= aradah.product %></td>
        <td><%= aradah.wells %></td>
        <td><%= aradah.from_thrawen_tank %></td>
        <td><%= aradah.product_total %></td>
        <td class="bg-light"><%= aradah.distribution %></td>
        <td class="bg-light"><%= aradah.networks %></td>
        <td><%= aradah.qnet_alqsmh %></td>
        <td><%= aradah.qnet_althrawen %></td>
        <td><%= aradah.qnet_althrawen_alatawlah %></td>
        <td><%= aradah.qnet_bneedwan1 %></td>
