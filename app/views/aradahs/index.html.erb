        <td><%= aradah.mnet_blhkm %></td>
        <td><%= aradah.mnet_aljawfaa %></td>
        <td class="bg-light"><%= aradah.mnet_total %></td>
        <td><%= aradah.bnet_almosa %></td>
        <td><%= aradah.bnet_qrnthbe %></td>
        <td class="bg-light"><%= aradah.bnet_total %></td>
        <td class="bg-light"><%= aradah.network_total %></td>
        <td class="bg-light"><%= aradah.almnahel %></td>
        <td><%= aradah.qshep_nkhal %></td>
        <td><%= aradah.qshep_alqsmh %></td>
