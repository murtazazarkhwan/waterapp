        <td><%= aradah.qshep_mashoqah %></td>
        <td class="bg-light"><%= aradah.qshep_total %></td>
        <td><%= aradah.bneshep_blkhzmr %></td>
        <td><%= aradah.bneshep_bnehasan %></td>
        <td class="bg-light"><%= aradah.bneshep_total %></td>
        <td><%= aradah.mshep_alhalah %></td>
        <td><%= aradah.mshep_dos %></td>
        <td class="bg-light"><%= aradah.mshep_total %></td>
        <td><%= aradah.bshep_almosa %></td>
        <td class="bg-light"><%= aradah.mnahel_total %></td>
        <td class="bg-light"><%= aradah.distripution_total %></td>
        <td><%= link_to 'Show', aradah %></td>
        <td><%= link_to 'Edit', edit_aradah_path(aradah) %></td>
        <td><%= link_to 'Destroy', aradah, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
